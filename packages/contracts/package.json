{
  "private": true,
  "name": "@cream/contracts",
  "version": "0.2.2",
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "watch": "tsc --watch",
    "build": "yarn compile",
    "compile": "truffle compile && yarn generate-types",
    "ganache": "./scripts/runGanache.sh",
    "generate-types": "typechain --target=truffle-v5 'build/contracts/*.json'",
    "migrate": "tsc -p ./tsconfig.migrate.json --outDir ./migrations && truffle migrate",
    "test": "NODE_OPTIONS=--max-old-space-size=4096 truffle test"
  },
  "author": "Kazuaki Ishiguro",
  "license": "ISC",
  "dependencies": {
    "@cream/circuits": "^0.2.0",
    "@cream/config": "^0.0.1",
    "@ethersproject/bignumber": "^5.0.14",
    "@openzeppelin/contracts": "3.2.0",
    "@truffle/artifactor": "^4.0.97",
    "@typechain/truffle-v5": "^4.0.1",
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "@types/web3": "^1.2.2",
    "cream-merkle-tree": "^0.0.7",
    "fs-extra": "^9.1.0",
    "libcream": "^0.1.2",
    "maci-cli": "0.4.11",
    "maci-contracts": "0.4.11",
    "maci-core": "0.4.11",
    "maci-crypto": "0.4.11",
    "maci-domainobjs": "0.4.11",
    "truffle": "5.1.65",
    "truffle-assertions": "^0.9.2",
    "typechain": "^4.0.1"
  },
  "devDependencies": {
    "ganache-cli": "^6.12.2"
  }
}
