{
  "extends": "solhint:recommended",
  "rules": {
	"compiler-version": ["error", "^0.6.12"],
	"func-name-mixedcase": "off",
	"var-name-mixedcase": "off",
	"no-empty-blocks": "off",
	"no-inline-assembly": "off",
	"not-rely-on-time": "off",
	"reason-string": [
	  "warn",
	  {
		"maxLength": 80
	  }
	]
  }
}
